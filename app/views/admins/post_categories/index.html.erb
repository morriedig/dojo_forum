<h1>This is post_category page</h1>

<%= form_for [:admins, @post_category], remote: true do |f| %>
  <%= f.label :title, "文章分類" %>
  <%= f.text_field :title %>
  <%= f.submit %>
<% end %>

<ul id= "all_post_category">
  <% @post_categories.each do |post_category| %>
    <%= render :partial => "admins/post_categories/category_list", locals: {post_category: post_category} %>
  <% end %>
</ul>